<style>
    .container {
	width: 80%;
	margin: 0 auto;
	display: flex;
	justify-content: space-between;
	align-items: center;
}

.product-image img {
	max-width: 100%;
	height: auto;
}

.product-details {
	flex: 1;
	margin-left: 50px;
}

.product-details h1 {
	font-size: 2rem;
	margin-bottom: 10px;
}

.price {
	font-size: 1.5rem;
	margin-bottom: 10px;
	color: #c00;
}

.description {
	margin-bottom: 20px;
}

form {
	display: flex;
	align-items: center;
}

label {
	margin-right: 10px;
}

input[type="number"] {
	width: 50px;
}

input[type="submit"] {
	background-color: #c00;
	color: #fff;
	padding: 10px 20px;
	border: none;
	cursor: pointer;
}

input[type="submit"]:hover {
	background-color: #ff0000;

}
</style>
