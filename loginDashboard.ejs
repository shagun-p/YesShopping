<html>
    <head>
        <style>
            #backButton {
              position: fixed;
              top: 0;
              left: 0;
            }
            
          </style>
        <title>Login Here</title>
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
    </head>
    <body class="container">
        <button id="backButton">Back</button>
  <script>
    document.getElementById('backButton').addEventListener('click', function() {
      window.history.back();
    });
  </script>
        <div class="page-header">
            <h1>Yes Shopping</h1>
        </div>
 
        <nav class="navbar navbar-default">
            <div class="container-fluid">
                <!-- Collect the nav links, forms, and other content for toggling -->
                <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                    <ul class="nav navbar-nav">
                      <li><a href="/home">Home</a></li>
                      <li><a href="/about">About</a></li>
                      <li><a href="/contact">Contact</a></li>
                      <li><a href="/help">Help</a></li>
                      <li><a href="/faq">FAQ</a></li>
                    </ul>
 
                    <ul class="nav navbar-nav navbar-right">
                      <li><a href="/orderList">Past orders</a></li>
                        <li><a href="/cart">Cart</a></li>
                        <li><a href="/wishlist">Wishlist</a></li>
                        <% if (name == "Log In") { %>
                        <li><a href="/login"><%= name %></a></li>
                        <% } else{ %>
                        <li><a href="/logout"><%= name %></a></li>
                        <% } %>
                       
                    </ul>
                </div>
            </div>
        </nav>
 
        <div class="container row">
          <h1>Welcome to YesShopping</h1>
          <p>This is your personalized dashboard page. You can view your account information, manage your settings, and access other features of our website from here.</p>
        </div>
    </body>
</html>
